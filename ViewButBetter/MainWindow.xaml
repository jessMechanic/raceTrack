<Window x:Class="ViewButBetter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ViewButBetter"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid OpacityMask="#FF310808" Background="#FF8ABB67">
        <Image x:Name="Image" OpacityMask="Black" Width="800" Margin="0,0,0,8" HorizontalAlignment="Left" RenderOptions.BitmapScalingMode="NearestNeighbor" />
    </Grid>
</Window>
